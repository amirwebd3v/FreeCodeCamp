{
  "version": 2,
  "builds": [
    {
      "src": "responsive-web-design/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "javascript-algorithms-and-data-structures/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "^/$",
      "dest": "/responsive-web-design/personal-portfolio-webpage.html"
    },
    {
      "src": "/responsive-web-design/(.*)",
      "dest": "/responsive-web-design/$1"
    },
    {
      "src": "/javascript-algorithms-and-data-structures/(.*)",
      "dest": "/javascript-algorithms-and-data-structures/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/responsive-web-design/$1"
    }
  ]
}